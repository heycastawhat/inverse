{
  "manifest_version": 3,
  "name": "Inverse",
  "version": "0.1",
  "description": "Lightweight extension to hide images.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "inverse.png",
      "48": "inverse.png",
      "128": "inverse.png"
    }
  },
  "permissions": ["activeTab", "scripting", "storage"]
}